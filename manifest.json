{
    "manifest_version": 3,
    "name": "Vanilla test",
    "description": "vanilla test.",
    "version": "1.0",
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      }
    },
    "permissions": ["activeTab", "scripting","storage"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content_utils.js",
               "content.js"]
      },
      {
        "matches": ["https://www.linkedin.com/jobs/view/*",
                    "https://www.linkedin.com/jobs/collections/*"],
        "js": ["job_extractor/job_extractor_utils.js",
               "job_extractor/job_extractor_collections.js",
               "job_extractor/job_extractor_views.js"]
      },
      {
        "matches": ["https://www.linkedin.com/jobs/view/*",
                    "https://www.linkedin.com/jobs/collections/*"],
        "js": ["keyword_analyzer/stop_words.js",
               "keyword_analyzer/keyword_utils.js"]
      }
    ],
    "background": {
      "service_worker": "service.js"
    }
  }
